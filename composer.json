{
    "name": "symfonyid/core-bundle",
    "type": "symfony-bundle",
    "description": "Provide toolkit for any project",
    "keywords": ["toolkit", "symfony2", "bundle", "ihsanudin"],
    "homepage": "https://github.com/SymfonyId/CoreBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Komunitas Symfony Indonesia",
            "homepage": "https://github.com/SymfonyId/CoreBundle"
        },
        {
            "name": "Muhammad Surya Ihsanuddin",
            "homepage": "https://github.com/ihsanudin"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "*",
        "doctrine/orm": "*",
        "doctrine/dbal": "*",
        "knplabs/knp-paginator-bundle": "~2.4",
        "symfonyid/symfony-bundle-plugins": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "5.1.*"
    },
    "autoload": {
        "psr-4": {
            "Symfonian\\Indonesia\\CoreBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}